// Static Addition

class Addition {
    public static void main(String[] args) {
       int a=10;
       int b=20;
       int sum=a+b;
       System.out.println(+sum);
    }
}


//  Dynamic Addiion

public class SumOfNumbers4  
{  
public static void main(String args[])  
{  
int x = Integer.parseInt(args[0]); 
int y = Integer.parseInt(args[1]);  
int sum = x + y;  
System.out.println("The sum of x and y is: " +sum);  
}  
}  
